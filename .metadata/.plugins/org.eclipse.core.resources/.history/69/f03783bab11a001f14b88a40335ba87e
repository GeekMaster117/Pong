package game;

import onlineFramework.Server;

public class HostServer extends Thread
{
private Server server;
	
	public ServerHandle()
	{
		this.server = new Server();
	}
	
	public void run()
	{
		this.server.start();
	}
	
	public void startServer()
	{
		this.start();
	}
	
	public void endServer()
	{
		this.server.end();
	}
}
